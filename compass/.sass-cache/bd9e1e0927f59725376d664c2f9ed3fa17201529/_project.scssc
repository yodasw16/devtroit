header[role="banner"] {
    margin: 0 auto;
    width: 90%;
    text-align: center;
    padding: 2em 0;
}

.mainFooter a {
    text-decoration: none;
}

[role="main"] {
    max-width: 88%;
    margin: 0 auto 2em;
}

.box {
    background-color: #fff;
    background-color: rgba(255, 255, 255, 0.9);
    @include box-shadow(rgba(0, 0, 0, 0.5) 0 7px 0);
    border-radius: 0 0 10px 10px;
    margin: 0 auto 2em;
    padding: .5em;
    @include single-transition(all,1s,ease);
    &:hover {
        background-color: #fff;
    }
}

.mainFooter {
    background-color: #4F89A1;
    @include box-shadow(rgba(0, 0, 0, 0.5) 0 -7px 0);
    color: #FFFFFF;
    padding: .5% 0;
    text-align: center;
}

.tweet ul {
    list-style-type: none;
    padding: 0
}

.tweet li {
    border-bottom: 1px dotted;
    padding: 10px 0;
}

.tweet li:first-child {
    padding-top: 0;
}

.tweet li:last-child {
    border: 0;
}

.simpleList {
    padding-left: 25px;
    margin: 0;
    @include column-width(16em);
}

.simpleList li {
    margin: 10px;
}

.badges {
    figure {
        margin: 15px 0;
        text-align: center;
    }
}

.twitters {
    display: table;
    iframe {
        vertical-align: text-top;
    }
}

form {
    fieldset {
        margin: 0 0 20px 0;
        label {
            display: block;
            margin: 0 0 5px 0;
            color: #4F89A1;
        }
        input.form-text {
            display: block;
            width: 95%;
            padding: 5px;
            font-size: 14px;
            border: 1px solid #999;
            background: #f5f5f5;
            @include box-shadow(rgba(0,0,0,.05) 0 5px 2px inset);
            border-radius: 4px;
            &:focus {
                border: 1px solid #ccc;
                background: #fff;
            }
        }
        p.form-help {
            margin: 5px 0 0 0;
            font-size: 12px;
            color: #999;
        }
    }
    fieldset.form-actions {
        margin: 0;
    }
    input[type="submit"] {
        margin: 0;
        padding: 5px 10px;
        font-size: 12px;
        font-weight: bold;
        border: 1px solid #999;
        background: #eee;
        border-radius: 4px;
        &:hover,&:focus {
            border: 1px solid #bbb;
            background: #e5e5e5;
        }
        &:active {
            border: 1px solid #ccc;
            background: #eee;
        }
    }
    input::-webkit-input-placeholder,input:-moz-placeholder {
        color: #ccc;
    }
    input:-moz-placeholder {
        color: #ccc;
    }
}

.success { color: green; }
.fail { color: red; }

